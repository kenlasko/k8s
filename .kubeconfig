apiVersion: v1
clusters:
- cluster:
    server: https://k3s.ucdialplans.com
  name: home-k3s
- cluster:
    server: https://cloud-k3s.ucdialplans.com
  name: cloud-k3s
contexts:
- context:
    cluster: home-k3s
    user: home-user
  name: home-context
- context:
    cluster: cloud-k3s
    user: cloud-user
  name: cloud-context
kind: Config
preferences:
  colors: true
users:
- name: home-user
  user:
    token: ***REMOVED***
- name: cloud-user
  user:
    token: cloud-token